# Production Environment Configuration
VITE_APP_ENV=production
VITE_APP_NAME=Soroban Ajo
VITE_APP_VERSION=0.1.0

# Stellar Network - Mainnet
VITE_STELLAR_NETWORK=mainnet
VITE_SOROBAN_RPC_URL=https://soroban-mainnet.stellar.org
VITE_HORIZON_URL=https://horizon.stellar.org
VITE_STELLAR_NETWORK_PASSPHRASE=Public Global Stellar Network ; September 2015

# Contract (update with production deployment)
VITE_SOROBAN_CONTRACT_ID=
VITE_CONTRACT_NETWORK=mainnet

# Wallet
VITE_DEFAULT_WALLET=freighter
VITE_WALLET_AUTO_CONNECT=false

# API
VITE_API_URL=https://api.soroban-ajo.com
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_DEBUG=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_MAINTENANCE_MODE=false

# UI
VITE_DEFAULT_THEME=light
VITE_ENABLE_ANIMATIONS=true
VITE_ITEMS_PER_PAGE=10

# Transactions
VITE_TX_TIMEOUT=30
VITE_MAX_TX_FEE=100000
VITE_DEFAULT_SLIPPAGE=0.5

# Monitoring (add your production keys)
VITE_SENTRY_DSN=
VITE_GA_ID=
VITE_MIXPANEL_TOKEN=

# Development Tools
VITE_ENABLE_DEVTOOLS=false
VITE_ENABLE_REDUX_DEVTOOLS=false
VITE_LOG_LEVEL=error

# Security
VITE_HTTPS=true
VITE_CORS_ORIGINS=https://soroban-ajo.com,https://www.soroban-ajo.com

# Performance
VITE_ENABLE_SW=true
VITE_CACHE_DURATION=3600

# Links
VITE_GITHUB_URL=https://github.com/Emmyt24/soroban-ajo
