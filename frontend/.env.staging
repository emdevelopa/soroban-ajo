# Staging Environment Configuration
VITE_APP_ENV=staging
VITE_APP_NAME=Soroban Ajo (Staging)
VITE_APP_VERSION=0.1.0

# Stellar Network - Testnet
VITE_STELLAR_NETWORK=testnet
VITE_SOROBAN_RPC_URL=https://soroban-testnet.stellar.org
VITE_HORIZON_URL=https://horizon-testnet.stellar.org
VITE_STELLAR_NETWORK_PASSPHRASE=Test SDF Network ; September 2015

# Contract (update with staging deployment)
VITE_SOROBAN_CONTRACT_ID=
VITE_CONTRACT_NETWORK=testnet

# Wallet
VITE_DEFAULT_WALLET=freighter
VITE_WALLET_AUTO_CONNECT=false

# API
VITE_API_URL=https://staging-api.soroban-ajo.com
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_DEBUG=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_MAINTENANCE_MODE=false

# UI
VITE_DEFAULT_THEME=light
VITE_ENABLE_ANIMATIONS=true
VITE_ITEMS_PER_PAGE=10

# Transactions
VITE_TX_TIMEOUT=30
VITE_MAX_TX_FEE=100000
VITE_DEFAULT_SLIPPAGE=0.5

# Monitoring (add your keys)
VITE_SENTRY_DSN=
VITE_GA_ID=
VITE_MIXPANEL_TOKEN=

# Development Tools
VITE_ENABLE_DEVTOOLS=false
VITE_ENABLE_REDUX_DEVTOOLS=false
VITE_LOG_LEVEL=info

# Security
VITE_HTTPS=true
VITE_CORS_ORIGINS=https://staging.soroban-ajo.com

# Performance
VITE_ENABLE_SW=true
VITE_CACHE_DURATION=1800

# Links
VITE_GITHUB_URL=https://github.com/Emmyt24/soroban-ajo
